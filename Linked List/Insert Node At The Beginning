// Definition of Linked List

class Node {
public:
     int data;
     Node* next;
     Node() : data(0), next(nullptr) {}
     Node(int x) : data(x), next(nullptr) {}
     Node(int x, Node* next) : data(x), next(next) {}
};

Node* insertAtFirst(Node* head, int newValue) {
    Node *temp = new Node(newValue);
    temp->next = head;
    head = temp;

    return list;
}
